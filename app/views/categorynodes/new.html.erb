<h2>New categorynode</h2>
<%= form_for(@categorynode) do |f| %>
  <div class="field">
    <%= f.label :title %><br />
    <%= f.text_field :title %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>

<%= link_to 'Back', {:controller => "categories", :action => "show", 
        :id => @category_id } %>
